package test;

public class test {

	public static void main(String[] args) {
		int n1 = 0;
		int n2= 1;
		int n3, i;
		int count = 10;
		int sum = 0;
		
		for(i = 2; i < count; i++) {
			n3 = n2 + n1;
			System.out.println(n3);
			if(count% 2 == 0) {
				sum += n3;
			}
			n1 = n2;
			n2 = n3;
		}
		System.out.print(sum);
	}
}
